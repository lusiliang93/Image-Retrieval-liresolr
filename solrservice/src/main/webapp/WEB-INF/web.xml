<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <!--RequestHandlerServlet-->
    <servlet>
        <servlet-name>ImgRepeat</servlet-name>
        <servlet-class>com.pingan.servlet.ImgRepeatServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImgRepeatDelete</servlet-name>
        <servlet-class>com.pingan.servlet.ImgDeleteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImgRepeatAdd</servlet-name>
        <servlet-class>com.pingan.servlet.ImgAddServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ImgRepeat</servlet-name>
        <url-pattern>/app/imgRepeat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImgRepeatDelete</servlet-name>
        <url-pattern>/app/imgRepeatDelete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImgRepeatAdd</servlet-name>
        <url-pattern>/app/imgRepeatAdd</url-pattern>
    </servlet-mapping>

</web-app>
